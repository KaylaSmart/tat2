  arrows: false,
  dots: false,
  customPaging: function(slider, i) {
    var thumbnail = $(slider.$slides[i]).data('thumbnail');
    return '<a href="javascript:void(0)"><img src="' + thumbnail + '"></a>';
  },
  responsive: [{
    breakpoint: 1152,
    settings: {
      arrows: true,
      slidesToShow: 3
    }
  }, {
    breakpoint: 900,
